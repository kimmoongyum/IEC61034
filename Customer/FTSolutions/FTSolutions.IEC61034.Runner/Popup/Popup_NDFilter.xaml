<bw:BaseIEC61034Popup x:Class="FTSolutions.IEC61034.Runner.Popup.Popup_NDFilter"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:sys="clr-namespace:System;assembly=mscorlib"
                 xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                 xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
                 xmlns:bw="clr-namespace:FTSolutions.IEC61034.Common.Base;assembly=FTSolutions.IEC61034.Common"
                 xmlns:lego="clr-namespace:eccFramework.SharedLib.Lego.Brick;assembly=eccFramework.SharedLib.Lego"
                 xmlns:conv="clr-namespace:FTSolutions.IEC61034.Common.Converter;assembly=FTSolutions.IEC61034.Common"
                 xmlns:util="clr-namespace:FTSolutions.IEC61034.Runner.Popup.Utility"
                 AllowsTransparency="True" WindowStyle="None" ResizeMode="NoResize"
                 WindowStartupLocation="CenterScreen"
                 Title="{DynamicResource main_header}" Width="1360" Height="1000">
    <bw:BaseIEC61034Popup.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/eccFramework.SharedLib.Core;component/Styles/PopupBrushes.xaml"/>
                <ResourceDictionary Source="/FTSolutions.IEC61034.Runner;component/Styles/IEC61034Brushes.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <x:Array x:Key="PolynomialItemSource" Type="sys:String">
                <sys:String>1</sys:String>
                <sys:String>2</sys:String>
                <sys:String>3</sys:String>
                <sys:String>4</sys:String>
            </x:Array>
            
            <conv:ReverseBooleanConverter x:Key="reverseBooleanConverter" />

            <Style x:Key="styleResultTitle" TargetType="lego:eccTextBlock">
                <Setter Property="Margin" Value="10,5" />
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontWeight" Value="Bold" />
                <Setter Property="VerticalAlignment" Value="Center" />
                <Setter Property="HorizontalAlignment" Value="Center" />
            </Style>

            <Style x:Key="styleFilterValue" TargetType="lego:eccTextBlock">
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontWeight" Value="Bold" />
                <Setter Property="VerticalAlignment" Value="Center" />
                <Setter Property="HorizontalAlignment" Value="Center" />
                <Setter Property="Foreground" Value="{StaticResource state_action_color}" />
            </Style>

            <Style x:Key="styleNDFilter" TargetType="lego:eccButton">
                <Setter Property="Margin" Value="2,0,10,0" />
                <Setter Property="Width" Value="70" />
                <Setter Property="Height" Value="42" />
                <Setter Property="FontSize" Value="12" />
                <Setter Property="FontWeight" Value="Bold" />
                <Setter Property="VerticalAlignment" Value="Center" />
            </Style>

            <Style x:Key="styleResultValue" TargetType="lego:eccTextBox">
                <Setter Property="FontWeight" Value="Bold" />
                <Setter Property="FontSize" Value="14" />
                <Setter Property="VerticalAlignment" Value="Center" />
                <Setter Property="HorizontalAlignment" Value="Stretch" />
                <Setter Property="HorizontalContentAlignment" Value="Left" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="Height" Value="34" />
                <Setter Property="Margin" Value="10,5" />
                <Setter Property="IsReadOnly" Value="True" />
                <Setter Property="TextMode" Value="Decimal" />
                <Setter Property="BorderBrush" Value="{StaticResource state_none_color}" />
                <Setter Property="Foreground" Value="{StaticResource state_valid_color}" />
            </Style>
        </ResourceDictionary>
    </bw:BaseIEC61034Popup.Resources>

    <DockPanel Background="{StaticResource popup_background}">
        <!-- HEADER -->
        <Border DockPanel.Dock="Top" Height="85" BorderThickness="1"
                BorderBrush="{StaticResource popup_header_bb}" Background="{StaticResource popup_header_bg}">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="150"/>
                    <ColumnDefinition />
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>

                <Border Grid.Column="0" BorderThickness="1" Background="White" Margin="10,16">
                    <Image Source="pack://application:,,,/eccFramework.SharedLib.Core;component/Images/ftsolutions_logo.png" 
                           RenderOptions.BitmapScalingMode="HighQuality" SnapsToDevicePixels="True" Margin="5" HorizontalAlignment="Right" />
                </Border>

                <StackPanel Grid.Column="1" Margin="0,10,0,0">
                    <TextBlock FontSize="20" Text="{DynamicResource popup_nd_filter_title}" Style="{StaticResource headerLabelTitleStyle}" />
                    <TextBlock Text="{DynamicResource popup_nd_filter_desc}" Style="{StaticResource headerLabelContentStyle}" TextWrapping="Wrap"/>
                </StackPanel>

                <StackPanel Grid.Column="2" Orientation="Horizontal">
                    <lego:ActionButton Content="{DynamicResource key_zero}" Width="70" Height="70" Margin="10,0,5,0" FontSize="14" BorderBrush="{StaticResource state_none_color}"
                                       Command="{Binding LaserDarkCommand}"/>
                    <lego:ActionButton Content="{DynamicResource key_span}" Width="70" Height="70" Margin="5,0,10,0" FontSize="14" BorderBrush="{StaticResource state_action_color}"
                                       Command="{Binding LaserClearCommand}"/>

                    <Border Margin="10,14" BorderThickness="1" Background="White">
                        <Image Source="/FTSolutions.IEC61034.Runner;component/Images/logo.png" Width="150" Margin="5"/>
                    </Border>
                </StackPanel>

            </Grid>
        </Border>
        <!-- FOOTER -->
        <Border DockPanel.Dock="Bottom" Height="70" Padding="0,4,0,0" BorderThickness="1" 
                BorderBrush="{StaticResource popup_footer_bb}" Background="{StaticResource popup_footer_bg}">
            <DockPanel>
                <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                    <ToggleButton x:Name="btnController" Background="Transparent" BorderThickness="0"
                                  Width="60" Margin="10,0,0,0"
                                  Style="{StaticResource SettingToggleButtonStyleKey}"
                                  ContentTemplate="{StaticResource SettingToggleButtonContentTemplateKey}"/>
                    <Popup IsOpen="{Binding ElementName=btnController, Path=IsChecked}"
                           PlacementTarget="{Binding ElementName=btnController}"
                           Placement="Right" StaysOpen="False" AllowsTransparency="True" HorizontalOffset="1" VerticalOffset="-3" >
                        <Border BorderBrush="#cF8c8c8c" BorderThickness="1" Background="#cF000000">
                            <util:UC_RemoteControl />
                        </Border>
                    </Popup>
                </StackPanel>

                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                    <lego:FancyButton Style="{StaticResource fancyButtonStyle}" Height="60" Command="{Binding SaveCommand}"
                                      FuncType="Save" Title="{DynamicResource btn_save}" SubTitle="{DynamicResource btn_save_desc}" />
                    <lego:FancyButton Style="{StaticResource fancyButtonStyle}" Height="60" Command="{Binding CancelClick}"
                                      FuncType="Cancel" Title="{DynamicResource btn_close}" SubTitle="{DynamicResource btn_close_desc}" />
                </StackPanel>
            </DockPanel>
        </Border>
        <!-- BODY -->
        <Grid Margin="5">
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="420"/>
            </Grid.RowDefinitions>

            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition Width="340"/>
                    <ColumnDefinition Width="520"/>
                </Grid.ColumnDefinitions>
                
                <TabControl Background="#2D2D30" BorderThickness="1" TabStripPlacement="Top" Margin="2,0"
                            ItemContainerStyle="{StaticResource chevronTabItemStyle}">
                    <lego:MainTabItem FontSize="12" Header="{DynamicResource key_transmission}" Width="172" Height="50" Background="{StaticResource popup_header_bg}">
                        <d3:ChartPlotter Margin="5" LegendVisibility="Collapsed" LegendVisible="False" FontSize="10"
                                         Background="{StaticResource graph_dark_bg}" Foreground="{StaticResource graph_dark_fg}" BorderBrush="{StaticResource graph_dark_bb}">
                            <d3:LineGraph DataSource="{Binding SeriesTransmission.SeriesDataSource}" Stroke="{StaticResource dark_series_color_1}" StrokeThickness="1.5" />
                            <d3:LineGraph DataSource="{Binding SeriesCorrectionTransmission.SeriesDataSource}" Stroke="{StaticResource dark_series_color_2}" StrokeThickness="1.5" />
                            
                            <d3:VerticalAxisTitle Content="{DynamicResource key_transmission}" FontSize="12" FontWeight="Bold"/>
                            <d3:HorizontalAxisTitle Content="{DynamicResource key_time_s}" FontSize="12" FontWeight="Bold"/>
                        </d3:ChartPlotter>
                    </lego:MainTabItem>
                </TabControl>
                <Grid Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,15,10,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <lego:eccCheckBox Grid.Row="0" IsChecked="True" Padding="2,0,0,0" IsEnabled="False">
                        <StackPanel Orientation="Horizontal">
                            <Rectangle Width="10" Height="10" Fill="{StaticResource dark_series_color_1}" Margin="0,0,2,0" />
                            <lego:eccTextBlock Text="{DynamicResource key_transmission2}" />
                        </StackPanel>
                    </lego:eccCheckBox>
                    <lego:eccCheckBox Grid.Row="1" IsChecked="True" Padding="2,0,0,0" IsEnabled="False">
                        <StackPanel Orientation="Horizontal">
                            <Rectangle Width="10" Height="10" Fill="{StaticResource dark_series_color_2}" Margin="0,0,2,0" />
                            <lego:eccTextBlock Text="{DynamicResource popup_nd_filter_calc_transmission}" />
                        </StackPanel>
                    </lego:eccCheckBox>
                </Grid>

                <TabControl Grid.Column="1" Background="#2D2D30" BorderThickness="1" TabStripPlacement="Top" Margin="2,0"
                            ItemContainerStyle="{StaticResource chevronTabItemStyle}">
                    <lego:MainTabItem FontSize="12" Header="{DynamicResource popup_nd_filter_least_square}" Width="252" Height="50" Background="{StaticResource popup_header_bg}">
                        <d3:ChartPlotter Margin="5" LegendVisibility="Collapsed" LegendVisible="False" FontSize="10"
                                         Background="{StaticResource graph_dark_bg}" Foreground="{StaticResource graph_dark_fg}" BorderBrush="{StaticResource graph_dark_bb}">
                            <d3:ElementMarkerPointsGraph DataSource="{Binding SeriesInputData.SeriesDataSource}">
                                <d3:ElementMarkerPointsGraph.Marker>
                                    <d3:CircleElementPointMarker Brush="Red" Fill="Red" Size="6" />
                                </d3:ElementMarkerPointsGraph.Marker>
                            </d3:ElementMarkerPointsGraph>
                            <d3:LineGraph StrokeThickness="1.5" DataSource="{Binding SeriesCertifiedData.SeriesDataSource}" Stroke="{StaticResource dark_series_color_2}" />
                            <d3:LineGraph StrokeThickness="1.5" DataSource="{Binding SeriesLeastSquareMethod.SeriesDataSource}" Stroke="{StaticResource dark_series_color_1}" />

                            <d3:VerticalAxisTitle Content="{DynamicResource key_transmission}" FontSize="12" FontWeight="Bold"/>
                            <d3:HorizontalAxisTitle Content="{DynamicResource key_transmission}" FontSize="12" FontWeight="Bold"/>

                        </d3:ChartPlotter>
                    </lego:MainTabItem>
                </TabControl>
                <Grid Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,15,10,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <lego:eccCheckBox Grid.Row="0" IsChecked="True" Padding="2,0,0,0" IsEnabled="False">
                        <StackPanel Orientation="Horizontal">
                            <Rectangle Width="10" Height="10" Fill="{StaticResource dark_series_color_1}" Margin="0,0,2,0" />
                            <lego:eccTextBlock Text="{DynamicResource popup_nd_filter_certi}" />
                        </StackPanel>
                    </lego:eccCheckBox>
                    <lego:eccCheckBox Grid.Row="1" IsChecked="True" Padding="2,0,0,0" IsEnabled="False">
                        <StackPanel Orientation="Horizontal">
                            <Rectangle Width="10" Height="10" Fill="{StaticResource dark_series_color_2}" Margin="0,0,2,0" />
                            <lego:eccTextBlock Text="{DynamicResource popup_nd_filter_calc}" />
                        </StackPanel>
                    </lego:eccCheckBox>
                </Grid>

                <DockPanel Grid.Column="2" VerticalAlignment="Center">
                    <lego:eccCheckBox DockPanel.Dock="Top" Content="{DynamicResource popup_nd_filter_no_compensation}" Foreground="{StaticResource state_valid_color}" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="10" Padding="10,0,0,0"
                                      IsChecked="{Binding IsNoCompensation}"/>

                    <Grid Margin="10" ShowGridLines="True">
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition Height="50"/>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="120" />
                            <ColumnDefinition Width="1*" />
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>

                        <lego:eccTextBlock Grid.Row="0" Grid.Column="1" Text="{DynamicResource monitor_curr_result}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBlock Grid.Row="0" Grid.Column="2" Text="{DynamicResource monitor_prev_result}" Style="{StaticResource styleResultTitle}" />

                        <lego:eccTextBlock Grid.Row="1" Text="{DynamicResource popup_nd_filter_polynomial}" Style="{StaticResource styleResultTitle}" />
                        <DockPanel Grid.Row="1" Grid.Column="1">
                            <lego:eccButton DockPanel.Dock="Right" Content="{DynamicResource btn_calculate}" Style="{StaticResource styleNDFilter}"
                                            Command="{Binding CalculateCommand}" />

                            <lego:eccComboBox FontSize="20" Height="42" Margin="10,0,2,0" ItemsSource="{StaticResource PolynomialItemSource}" 
                                              SelectedValue="{Binding POLYNIMIAL}"/>

                        </DockPanel>
                        <lego:eccTextBlock Grid.Row="1" Grid.Column="2" Style="{StaticResource styleResultTitle}" 
                                           Text="{Binding SESSION_MANAGER.LightPolynomialInfo.Polynomial}" Foreground="{StaticResource state_action_color}"/>

                        <lego:eccTextBlock Grid.Row="2" Text="{DynamicResource popup_nd_filter_4th_term}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBox Grid.Row="2" Grid.Column="1" Style="{StaticResource styleResultValue}" 
                                         Text="{Binding COEF_4}"/>
                        <lego:eccTextBox Grid.Row="2" Grid.Column="2" Style="{StaticResource styleResultValue}"
                                           Text="{Binding SESSION_MANAGER.LightPolynomialInfo.Coefficient4}" Foreground="{StaticResource state_action_color}"/>

                        <lego:eccTextBlock Grid.Row="3" Text="{DynamicResource popup_nd_filter_3rd_term}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBox Grid.Row="3" Grid.Column="1" Style="{StaticResource styleResultValue}" 
                                         Text="{Binding COEF_3}"/>
                        <lego:eccTextBox Grid.Row="3" Grid.Column="2" Style="{StaticResource styleResultValue}"
                                           Text="{Binding SESSION_MANAGER.LightPolynomialInfo.Coefficient3}" Foreground="{StaticResource state_action_color}"/>

                        <lego:eccTextBlock Grid.Row="4" Text="{DynamicResource popup_nd_filter_2nd_term}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBox Grid.Row="4" Grid.Column="1" Style="{StaticResource styleResultValue}" 
                                         Text="{Binding COEF_2}"/>
                        <lego:eccTextBox Grid.Row="4" Grid.Column="2" Style="{StaticResource styleResultValue}"
                                           Text="{Binding SESSION_MANAGER.LightPolynomialInfo.Coefficient2}" Foreground="{StaticResource state_action_color}"/>

                        <lego:eccTextBlock Grid.Row="5" Text="{DynamicResource popup_nd_filter_1st_term}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBox Grid.Row="5" Grid.Column="1" Style="{StaticResource styleResultValue}" 
                                         Text="{Binding COEF_1}"/>
                        <lego:eccTextBox Grid.Row="5" Grid.Column="2" Style="{StaticResource styleResultValue}"
                                           Text="{Binding SESSION_MANAGER.LightPolynomialInfo.Coefficient1}" Foreground="{StaticResource state_action_color}"/>

                        <lego:eccTextBlock Grid.Row="6" Text="{DynamicResource popup_nd_filter_intercept}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBox Grid.Row="6" Grid.Column="1" Style="{StaticResource styleResultValue}" 
                                         Text="{Binding Y_INTERCEPT}"/>
                        <lego:eccTextBox Grid.Row="6" Grid.Column="2" Style="{StaticResource styleResultValue}"
                                           Text="{Binding SESSION_MANAGER.LightPolynomialInfo.YIntercept}" Foreground="{StaticResource state_action_color}"/>
                    </Grid>
                </DockPanel>
            </Grid>

            <Border Grid.Row="1" BorderThickness="3,1,3,3" Margin="2,5,2,2" BorderBrush="DarkGoldenrod" Background="Black" ClipToBounds="True">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="640"/>
                    </Grid.ColumnDefinitions>

                    <Viewbox Grid.Row="0" Grid.RowSpan="2" Grid.Column="0" ClipToBounds="True" Margin="10" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <util:UC_StepMotor RenderOptions.BitmapScalingMode="HighQuality" SnapsToDevicePixels="True"/>
                    </Viewbox>

                    <lego:eccCheckBox x:Name="chkCalibration" Grid.Row="0" Grid.Column="1" Content="{DynamicResource popup_nd_filter_update_filter}" Foreground="{StaticResource state_valid_color}" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="10,0" Padding="10,0,0,0"/>

                    <Grid Grid.Row="0" Grid.RowSpan="2" Grid.Column="1" VerticalAlignment="Center" Margin="5" ShowGridLines="True">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="40"></RowDefinition>
                            <RowDefinition Height="45"></RowDefinition>
                            <RowDefinition Height="45"></RowDefinition>
                            <RowDefinition Height="45"></RowDefinition>
                            <RowDefinition Height="45"></RowDefinition>
                            <RowDefinition Height="45"></RowDefinition>
                            <RowDefinition Height="45"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="100"></ColumnDefinition>
                            <ColumnDefinition Width="140"></ColumnDefinition>
                            <ColumnDefinition Width="140"></ColumnDefinition>
                            <ColumnDefinition Width="140"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <lego:eccTextBlock Grid.Row="0" Grid.Column="0" Text="{DynamicResource key_filter}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBlock Grid.Row="0" Grid.Column="1" Text="{DynamicResource popup_nd_filter_theoretical}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBlock Grid.Row="0" Grid.Column="2" Text="{DynamicResource popup_nd_filter_certi}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBlock Grid.Row="0" Grid.Column="3" Text="{DynamicResource key_measure}" Style="{StaticResource styleResultTitle}" />
                        <lego:eccTextBlock Grid.Row="0" Grid.Column="4" Text="{DynamicResource popup_nd_filter_error}" Style="{StaticResource styleResultTitle}" />

                        <lego:eccTextBlock Grid.Row="1" Grid.Column="0" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo1.FILTER_NO}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="1" Grid.Column="1" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo1.CALC_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBox Grid.Row="1" Grid.Column="2" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo1.CERTI_TRANSMISSION}" 
                                         Style="{StaticResource styleResultValue}" HorizontalContentAlignment="Center" FontSize="18"
                                         IsReadOnly="{Binding Path=IsChecked, ElementName=chkCalibration, Converter={StaticResource reverseBooleanConverter}}"/>
                        <lego:eccTextBlock Grid.Row="1" Grid.Column="3" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo1.MEASURE_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="1" Grid.Column="4" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo1.ERROR_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />

                        <lego:eccTextBlock Grid.Row="2" Grid.Column="0" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo2.FILTER_NO}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="2" Grid.Column="1" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo2.CALC_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBox Grid.Row="2" Grid.Column="2" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo2.CERTI_TRANSMISSION}" 
                                         Style="{StaticResource styleResultValue}" HorizontalContentAlignment="Center" FontSize="18"
                                         IsReadOnly="{Binding Path=IsChecked, ElementName=chkCalibration, Converter={StaticResource reverseBooleanConverter}}"/>
                        <lego:eccTextBlock Grid.Row="2" Grid.Column="3" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo2.MEASURE_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="2" Grid.Column="4" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo2.ERROR_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />

                        <lego:eccTextBlock Grid.Row="3" Grid.Column="0" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo3.FILTER_NO}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="3" Grid.Column="1" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo3.CALC_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBox Grid.Row="3" Grid.Column="2" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo3.CERTI_TRANSMISSION}" 
                                         Style="{StaticResource styleResultValue}" HorizontalContentAlignment="Center" FontSize="18"
                                         IsReadOnly="{Binding Path=IsChecked, ElementName=chkCalibration, Converter={StaticResource reverseBooleanConverter}}"/>
                        <lego:eccTextBlock Grid.Row="3" Grid.Column="3" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo3.MEASURE_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="3" Grid.Column="4" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo3.ERROR_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />

                        <lego:eccTextBlock Grid.Row="4" Grid.Column="0" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo4.FILTER_NO}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="4" Grid.Column="1" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo4.CALC_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBox Grid.Row="4" Grid.Column="2" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo4.CERTI_TRANSMISSION}" 
                                         Style="{StaticResource styleResultValue}" HorizontalContentAlignment="Center" FontSize="18"
                                         IsReadOnly="{Binding Path=IsChecked, ElementName=chkCalibration, Converter={StaticResource reverseBooleanConverter}}"/>
                        <lego:eccTextBlock Grid.Row="4" Grid.Column="3" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo4.MEASURE_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="4" Grid.Column="4" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo4.ERROR_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />

                        <lego:eccTextBlock Grid.Row="5" Grid.Column="0" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo5.FILTER_NO}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="5" Grid.Column="1" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo5.CALC_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBox Grid.Row="5" Grid.Column="2" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo5.CERTI_TRANSMISSION}" 
                                         Style="{StaticResource styleResultValue}" HorizontalContentAlignment="Center" FontSize="18"
                                         IsReadOnly="{Binding Path=IsChecked, ElementName=chkCalibration, Converter={StaticResource reverseBooleanConverter}}"/>
                        <lego:eccTextBlock Grid.Row="5" Grid.Column="3" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo5.MEASURE_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="5" Grid.Column="4" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo5.ERROR_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />

                        <lego:eccTextBlock Grid.Row="6" Grid.Column="0" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo6.FILTER_NO}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="6" Grid.Column="1" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo6.CALC_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBox Grid.Row="6" Grid.Column="2" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo6.CERTI_TRANSMISSION}" 
                                         Style="{StaticResource styleResultValue}" HorizontalContentAlignment="Center" FontSize="18"
                                         IsReadOnly="{Binding Path=IsChecked, ElementName=chkCalibration, Converter={StaticResource reverseBooleanConverter}}"/>
                        <lego:eccTextBlock Grid.Row="6" Grid.Column="3" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo6.MEASURE_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                        <lego:eccTextBlock Grid.Row="6" Grid.Column="4" Text="{Binding SESSION_MANAGER.NDFilterCollection.FilterNo6.ERROR_TRANSMISSION}" Style="{StaticResource styleFilterValue}" />
                    </Grid>
                </Grid>
            </Border>
        </Grid>
    </DockPanel>
</bw:BaseIEC61034Popup>
